{
  "name": "ped5-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4205",
    "start-prod": "ng serve --configuration production",
    "build": "rm -rf dist/* && ng build --configuration production --build-optimizer",
    "deploy-dev": "(colC='\\033[0;36m';colN='\\033[0m'; echo ${colC}Running npm build ...${colN} && npm run build && echo ${colC}Rsyncing dist ...${colN} && rsync -avh --delete dist biocomp@persefone:/var/www/ped5-dev/ && echo ${colC}Done with deploy-dev!${colC})",
    "deploy-prod": "(colC='\\033[0;36m';colN='\\033[0m'; echo ${colC}Running npm build ...${colN} && npm run build && echo ${colC}Rsyncing dist ...${colN} && rsync -avh --delete dist biocomp@persefone:/var/www/ped5/ && echo ${colC}Done with deploy-prod!${colC})"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.7",
    "@angular/cdk": "^11.2.12",
    "@angular/common": "~12.2.7",
    "@angular/compiler": "~12.2.7",
    "@angular/core": "~12.2.7",
    "@angular/forms": "~12.2.7",
    "@angular/platform-browser": "~12.2.7",
    "@angular/platform-browser-dynamic": "~12.2.7",
    "@angular/router": "~12.2.7",
    "@popperjs/core": "^2.9.2",
    "EBI-Icon-fonts": "^1.3.1",
    "bootstrap": "^5.0.1",
    "bootstrap-icons": "^1.5.0",
    "feature-viewer-typescript": "^2.2.5",
    "molstar": "^2.0.6",
    "ngx-bootstrap": "^6.2.0",
    "pdbe-molstar": "^1.1.0-dev.4",
    "proseqviewer": "^1.0.84",
    "rxjs": "~6.6.0",
    "swagger-ui": "^3.52.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular/cli": "~12.2.7",
